<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="antrcp-template">
         <property
               name="lifeCycleURI"
               value="bundleclass://antzou.template.application/com.antzou.application.E4LifeCycle">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/antzou.template.application/css/default.css">
         </property>
         <property
               name="appName"
               value="antrcp-template">
         </property>
      </product>
   </extension>
   
    <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.antzou.application.handlers.GeneralPreferencePage"
            id="com.example.e4.rcp.handlers.GeneralPreferencePage"
            name="常规">
      </page>
   </extension>

	<extension point="org.eclipse.core.runtime.preferences">
	    <initializer 
	        class="com.antzou.application.handlers.PreferenceInitializer"
	        id="com.example.e4.rcp.preferenceInitializer"/>
	</extension>
</plugin>
