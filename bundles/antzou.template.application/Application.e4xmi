<?xml version="1.0" encoding="UTF-8"?>
<application:Application xmi:version="2.0"
	xmlns:xmi="http://www.omg.org/XMI"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:application="http://www.eclipse.org/ui/2010/UIModel/application"
	xmlns:basic="http://www.eclipse.org/ui/2010/UIModel/application/ui/basic"
	xmlns:menu="http://www.eclipse.org/ui/2010/UIModel/application/ui/menu"
	xmi:id="app_root"
	elementId="antzou.template.application"
	bindingContexts="root_context">
	<children xsi:type="basic:TrimmedWindow"
		xmi:id="main_window"
		elementId="antzou.template.application.window.main"
		label="antrcp-Template" >
		<!-- 可分割的布局容器 -->
		<children xsi:type="basic:PartSashContainer"
			xmi:id="main_sash_container"
			elementId="antzou.template.application.main.sashcontainer" horizontal="true">

			<!-- 左侧导航区域 -->
			<children xsi:type="basic:PartStack"
				xmi:id="left_nav_stack"
				elementId="antzou.template.application.left.navstack" containerData="1">
				<children xsi:type="basic:Part" xmi:id="left_nav_part"
					elementId="antzou.template.application.left.navpart"
					contributionURI="bundleclass://antzou.template.application/com.antzou.application.parts.NavigationPart"
					iconURI="platform:/plugin/antzou.template.application/icons/nevigation.png"
					label="操作导航" 
					closeable="false"/>
			</children>

			<!-- 右侧编辑器区域 -->
			<children xsi:type="basic:PartStack"
				xmi:id="right_editor_stack"
				elementId="antzou.template.application.right.editorstack" containerData="3">
				<children xsi:type="basic:Part"
					xmi:id="default_editor_part"
					elementId="com.antzou.application.right.editorpart"
					contributionURI="bundleclass://antzou.template.application/com.antzou.application.parts.DefaultEditorPart"
					iconURI="platform:/plugin/antzou.template.application/icons/save_edit.png"
					label="Welcome" />
					
				<children xsi:type="basic:Part"
				    xmi:id="input_part"
				    elementId="com.antzou.application.parts.input"
				    contributionURI="bundleclass://antzou.template.application/com.antzou.application.parts.InputPart"
				    iconURI="platform:/plugin/antzou.template.application/icons/save_edit.png"
				    label="信息录入"
				    closeable="true" 
				    toBeRendered="false"/>
				
				<children xsi:type="basic:Part"
				    xmi:id="query_part"
				    elementId="com.antzou.application.parts.query"
				    contributionURI="bundleclass://antzou.template.application/com.antzou.application.parts.QueryPart"
				    iconURI="platform:/plugin/antzou.template.application/icons/save_edit.png"
				    label="信息查询"
				    closeable="true" 
				    toBeRendered="false"/>
				
				<children xsi:type="basic:Part"
				    xmi:id="edit_part"
				    elementId="com.antzou.application.parts.edit"
				    contributionURI="bundleclass://antzou.template.application/com.antzou.application.parts.EditPart"
				    iconURI="platform:/plugin/antzou.template.application/icons/save_edit.png"
				    label="信息修改"
				    closeable="true" 
				    toBeRendered="false"/>
			</children>
		</children>

		<!-- 菜单栏 -->
		<mainMenu xmi:id="main_menu"
			elementId="org.eclipse.ui.main.menu">
			<children xsi:type="menu:Menu"
				xmi:id="file_menu" elementId="file" label="File">
				<children xsi:type="menu:HandledMenuItem"
					xmi:id="file_open_item"
					elementId="com.antzou.application.handleditem.file.open" label="Open"
					iconURI="platform:/plugin/antzou.template.application/icons/open_in_app.png"
					command="cmd_open" />
				<children xsi:type="menu:HandledMenuItem"
					xmi:id="file_save_item"
					elementId="com.antzou.application.handleditem.save" label="Save"
					iconURI="platform:/plugin/antzou.template.application/icons/save_edit.png"
					command="cmd_save" />
				<children xsi:type="menu:HandledMenuItem"
					xmi:id="file_quit_item"
					elementId="com.antzou.application.handleditem.quit" label="Quit"
					command="cmd_quit" />
			</children>
			<children xsi:type="menu:Menu"
			    xmi:id="window_menu"
			    elementId="window.menu"
			    label="Window">
			    <children xsi:type="menu:Menu"
			        xmi:id="appearance_menu"
			        elementId="window.appearance.menu"
			        label="Appearance">
			        <children xsi:type="menu:HandledMenuItem"
			            xmi:id="hide_toolbar_item"
			            elementId="window.appearance.hidetoolbar"
			            label="Hide Toolbar"
			            command="cmd_hide_toolbar"/>
			            
			        <children xsi:type="menu:HandledMenuItem"
			            xmi:id="hide_statusbar_item"
			            elementId="window.appearance.hidestatusbar"
			            label="Hide Status Bar"
			            command="cmd_hide_statusbar"/>
			            
			        <children xsi:type="menu:HandledMenuItem"
			            xmi:id="toggle_fullscreen_item"
			            elementId="window.appearance.fullscreen"
			            label="Toggle Full Screen"
			            command="cmd_toggle_fullscreen"/>
			    </children>
			    <!-- 添加首选项菜单项 -->
			    <children xsi:type="menu:HandledMenuItem"
			        xmi:id="preferences_menu_item"
			        elementId="com.antzou.application.preferences"
			        label="Preferences"
			        command="cmd_preferences"/>
			</children>
			<children xsi:type="menu:Menu"
			    xmi:id="help_menu" elementId="help" label="Help">
			    <children xsi:type="menu:HandledMenuItem"
			        xmi:id="about_menu_item"
			        elementId="org.eclipse.ui.help.aboutAction"
			        label="About"
			        command="cmd_about"/>
			</children>
		</mainMenu>

		<!-- 工具栏 -->
		<trimBars xmi:id="main_toolbar"
			elementId="antzou.template.application.trimbar.top">
			<children xsi:type="menu:ToolBar"
				xmi:id="toolbar"
				elementId="org.eclipse.ui.main.toolbar">
				<children xsi:type="menu:HandledToolItem"
					xmi:id="toolbar_save_item"
					elementId="com.antzou.application.handleditem.trimbar.top.save"
					iconURI="platform:/plugin/antzou.template.application/icons/save_edit.png"
					command="cmd_save" />
				<children xsi:type="menu:HandledToolItem"
					xmi:id="toolbar_open_item"
					elementId="com.antzou.application.handleditem.trimbar.top.open"
					iconURI="platform:/plugin/antzou.template.application/icons/open_in_app.png"
					command="cmd_open" />
			</children>
		</trimBars>
		
		<!-- Statusbar -->
		<trimBars xmi:id="status_bar"
		    elementId="antzou.template.application.statusbar"
		    side="Bottom">
		    
		    <children xsi:type="menu:ToolControl"
		        xmi:id="memory_status_control"
		        elementId="com.antzou.application.statusbar.memory"
		        contributionURI="bundleclass://antzou.template.application/com.antzou.application.controls.MemoryStatusControl"
		        visible="true">
		    </children>
		    
		</trimBars>
	</children>

	<!-- 在命令部分添加 -->
	<commands xmi:id="cmd_preferences"
	    elementId="org.eclipse.ui.window.preferences"
	    commandName="Preferences"/>
	
	<!-- 在处理器部分添加 -->
	<handlers xmi:id="handler_preferences"
	    elementId="com.antzou.application.preferences.handler"
	    contributionURI="bundleclass://antzou.template.application/com.antzou.application.handlers.OpenPreferencesHandler"
	    command="cmd_preferences"/>
	
	<!-- 在命令部分 -->
	<commands xmi:id="cmd_about"
	    elementId="org.eclipse.ui.help.aboutAction"
	    commandName="About"/>
	
	<!-- 在处理器部分 -->
	<handlers xmi:id="handler_about"
    elementId="com.antzou.application.aboutHandler"
    contributionURI="bundleclass://antzou.template.application/com.antzou.application.handlers.OpenAboutHandler"
    command="cmd_about"/>

	<handlers xmi:id="handler_quit"
		elementId="com.antzou.application.handler.quitCommand"
		contributionURI="bundleclass://antzou.template.application/com.antzou.application.handlers.QuitHandler"
		command="cmd_quit" />
	<handlers xmi:id="handler_open"
		elementId="com.antzou.application.handler.openCommand"
		contributionURI="bundleclass://antzou.template.application/com.antzou.application.handlers.OpenHandler"
		command="cmd_open"></handlers>
	<handlers xmi:id="handler_save"
		elementId="com.antzou.application.handler.saveCommand"
		contributionURI="bundleclass://antzou.template.application/com.antzou.application.handlers.SaveHandler"
		command="cmd_save" />
	
	<handlers xmi:id="handler_hide_toolbar"
	    elementId="com.antzou.application.handler.hidetoolbar"
	    contributionURI="bundleclass://antzou.template.application/com.antzou.application.handlers.ToolbarVisibilityHandler"
	    command="cmd_hide_toolbar"/>
	<handlers xmi:id="handler_hide_statusbar"
	    elementId="com.antzou.application.handler.hidestatusbar"
	    contributionURI="bundleclass://antzou.template.application/com.antzou.application.handlers.StatusbarVisibilityHandler"
	    command="cmd_hide_statusbar"/>
	<handlers xmi:id="handler_toggle_fullscreen"
	    elementId="com.antzou.application.handler.togglefullscreen"
	    contributionURI="bundleclass://antzou.template.application/com.antzou.application.handlers.FullScreenHandler"
	    command="cmd_toggle_fullscreen"/>

	<rootContext xmi:id="root_context"
		elementId="org.eclipse.ui.contexts.dialogAndWindow"
		name="In Dialog and Windows">
		<children xmi:id="window_context"
			elementId="org.eclipse.ui.contexts.window" name="In Windows" />
		<children xmi:id="dialog_context"
			elementId="org.eclipse.ui.contexts.dialog" name="In Dialogs" />
	</rootContext>

	<commands xmi:id="cmd_quit"
		elementId="org.eclipse.ui.file.exit" commandName="quitCommand" />
	<commands xmi:id="cmd_open"
		elementId="org.eclipse.ui.file.open" commandName="openCommand" />
	<commands xmi:id="cmd_save"
		elementId="org.eclipse.ui.file.save" commandName="saveCommand" />
	<commands xmi:id="cmd_hide_toolbar"
	    elementId="org.eclipse.ui.window.hide.toolbar"
	    commandName="Hide Toolbar"/>
	<commands xmi:id="cmd_hide_statusbar"
	    elementId="org.eclipse.ui.window.hide.statusbar"
	    commandName="Hide Status Bar"/>
	<commands xmi:id="cmd_toggle_fullscreen"
	    elementId="org.eclipse.ui.window.toggle.fullscreen"
	    commandName="Toggle Full Screen"/>
	
	<addons xmi:id="addon_commands"
		elementId="org.eclipse.e4.core.commands.service"
		contributionURI="bundleclass://org.eclipse.e4.core.commands/org.eclipse.e4.core.commands.CommandServiceAddon" />
	<addons xmi:id="addon_contexts"
		elementId="org.eclipse.e4.ui.contexts.service"
		contributionURI="bundleclass://org.eclipse.e4.ui.services/org.eclipse.e4.ui.services.ContextServiceAddon" />
	<addons xmi:id="addon_bindings"
		elementId="org.eclipse.e4.ui.bindings.service"
		contributionURI="bundleclass://org.eclipse.e4.ui.bindings/org.eclipse.e4.ui.bindings.BindingServiceAddon" />
	<addons xmi:id="addon_command_processing"
		elementId="org.eclipse.e4.ui.workbench.commands.model"
		contributionURI="bundleclass://org.eclipse.e4.ui.workbench/org.eclipse.e4.ui.internal.workbench.addons.CommandProcessingAddon" />
	<addons xmi:id="addon_handler_processing"
		elementId="org.eclipse.e4.ui.workbench.handler.model"
		contributionURI="bundleclass://org.eclipse.e4.ui.workbench/org.eclipse.e4.ui.internal.workbench.addons.HandlerProcessingAddon" />
	<addons xmi:id="addon_context_processing"
		elementId="org.eclipse.e4.ui.workbench.contexts.model"
		contributionURI="bundleclass://org.eclipse.e4.ui.workbench/org.eclipse.e4.ui.internal.workbench.addons.ContextProcessingAddon" />
	<addons xmi:id="addon_binding_processing"
		elementId="org.eclipse.e4.ui.workbench.bindings.model"
		contributionURI="bundleclass://org.eclipse.e4.ui.workbench.swt/org.eclipse.e4.ui.workbench.swt.util.BindingProcessingAddon" />
</application:Application>